platform :ios, '13.0'

target 'AiBC' do
  use_frameworks!

  # Pods for AiBC
  pod 'Firebase'
  pod 'Firebase/Core'
  pod 'Firebase/Firestore'
  pod 'Firebase/Auth'
  pod 'Firebase/Analytics'
  pod 'Kingfisher'
  pod 'Alamofire'
  pod 'lottie-ios'
  pod 'FaveButton'

  target 'AiBCTests' do
      inherit! :search_paths
      pod 'Firebase'
  end
end